<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>MVCapproach2DB</display-name>
  
<welcome-file-list>
    <welcome-file>MVCapproach2DB/WebContent/Welcome.jsp</welcome-file>
</welcome-file-list>
  
<servlet>
	<servlet-name>DataBaseServlet</servlet-name>
    <servlet-class>servletBean.DataBaseServlet</servlet-class>
</servlet>   

<servlet>
	<servlet-name>SecondColumn</servlet-name>
	<servlet-class>servletBean.SecondColumn</servlet-class>
</servlet>
  
<servlet-mapping>
	<servlet-name>DataBaseServlet</servlet-name>
    <url-pattern>/DataBaseServlet</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>SecondColumn</servlet-name>
    <url-pattern>/SecondColumn</url-pattern>
</servlet-mapping>

</web-app>

